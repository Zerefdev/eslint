{
  "env": {
    "browser": true,
    "commonjs": true,
    "es2020": true,
    "node": true
  },
  "extends": [
    "plugin:vue/essential",
    "standard"
  ],
  "parserOptions": {
    "ecmaVersion": 11
  },
  "plugins": [
    "vue"
  ],
  "rules": {
    "space-before-function-paren": 0,
    "new-cap": 1
  }
}